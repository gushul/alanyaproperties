= simple_form_for [:admin, @admin_feedback], :html => {:class => 'form-horizontal'} do |f|
  = f.input :name
  = f.input :email
  = f.input :message
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), |
      admin_manager_people_path, :class => 'btn'                   |
