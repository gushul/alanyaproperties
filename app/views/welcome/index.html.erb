<div class="offers clearfix">
  <% @properties.each do |property| %>
    <%= link_to property, class: "offer_link#{' new' if property.new? }" do %>
      <%= image_tag property.photo.thumb2 %>
      <% if property.hot? %>
        <i class="ribbon_plus_orange plus"></i>
        <i class="ribbon_hot_dial"></i>
      <% elsif property.new? %>
        <i class="ribbon_plus_blue plus"></i>
        <i class="ribbon_new"></i>
      <% else %>
        <i class="ribbon_plus_orange plus"></i>
      <% end -%>

      <span class="offer_info clearfix">
        <span class="price"><%= number_to_currency property.price, 'euro' %></span>
        <span class="title"><%= property.city.name %></span>
        <%= property.name %>
      </span>
    <% end %>
  <% end %>
</div>

