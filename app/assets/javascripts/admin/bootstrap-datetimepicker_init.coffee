$ ->
  $('.datetimepicker').datetimepicker
    autoclose: true
    language: 'ru'
    weekStart: 1
    todayBtn: true
    format: 'yyyy-mm-dd hh:ii:ss'
    todayHighlight: true

  $('.datepicker').datetimepicker
    autoclose: true
    language: 'ru'
    weekStart: 1
    todayBtn: true
    format: 'yyyy-mm-dd'
    todayHighlight: true
    viewSelect: 'month'
    minView: 2
